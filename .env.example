# Your LMS username
LMS_USERNAME=your_username_here
# Your LMS password
LMS_PASSWORD=your_password_here
# Output directory for extracted MP4 files
OUTPUT_DIR=extracted
# Run browser in headless mode (true/false), default: false which opens the browser window
HEADLESS=false
# Download directory for .rar files (default: downloads)
DOWNLOADS_DIR=downloads
# Timeout for page loads in milliseconds (default: 180000 = 3 minutes)
TIMEOUT_PAGE_LOAD=180000
# Timeout for downloads in milliseconds (default: 3600000 = 1 hour)
DOWNLOAD_TIMEOUT=3600000
# Log level (INFO, DEBUG, WARNING, ERROR)
LOG_LEVEL=INFO
